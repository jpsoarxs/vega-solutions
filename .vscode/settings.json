{
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#46211f",
		"activityBar.activeBorder": "#adadad",
		"activityBar.background": "#16100f",
		"activityBar.foreground": "#808080",
		"activityBar.inactiveForeground": "#b6b6b6",
		"activityBarBadge.background": "#46211f",
		"activityBarBadge.foreground": "#808080",
		"sash.hoverBorder": "#46211f",
		"statusBar.background": "#aa2f2f",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#46211f",
		"statusBarItem.remoteBackground": "#aa2f2f",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#aa2f2f",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#aa2f2f99",
		"titleBar.inactiveForeground": "#e7e7e799"
	},
	"js/ts.implicitProjectConfig.experimentalDecorators": true,
	"editor.formatOnSave": true,
	"editor.tabSize": 2,
	"editor.rulers": [
		80
	],
	"eslint.validate": [
		"javascript",
		"typescript"
	],
	"eslint.alwaysShowStatus": true,
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"eslint.codeAction.showDocumentation": {
		"enable": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.format.enable": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"[typescript]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "never",
	"javascript.format.semicolons": "remove",
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"terminal.integrated.defaultProfile.linux": "zsh",
	"cSpell.enabled": false
}